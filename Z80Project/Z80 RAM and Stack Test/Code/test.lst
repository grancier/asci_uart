0008: C30000   [44]     			JP		MAIN
                        			
000B:                   INIT_UART:
000B: 3E00     [ 7]                 ld	a,0
000D: 0601     [14]     			ld	b,1
000F: 0E02     [21]     			ld	c,2
0011: 1603     [28]     			ld	d,#3
0013: 1E04     [35]     			ld	e,4
0015: 2605     [42]     			ld	h,5
0017: 2E06     [49]     			ld	l,6
0019: 213412   [59]     			ld	hl,1234h
001C: 3607     [69]     			ld	(hl),7
001E: DB21     [80]     			in	a,(33)
0020: D321     [91]     			out	(33),a
0022: C9       [101]    			RET
                        			
